{
  "name": "shop-service",
  "version": "0.0.1",
  "description": "A back end for a POS system",
  "author": "Jonas Groenbek",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm-with-datasource": "typeorm-ts-node-esm -d src/postgres/typeorm.datasource.ts",
    "typeorm-with-test-datasource": "typeorm-ts-node-esm -d src/postgres/typeorm-test.datasource.ts",
    "typeorm-without-datasource": "typeorm-ts-node-esm",
    "migration:create": "npm run typeorm-without-datasource migration:create",
    "migration:generate": "npm run build && npm run typeorm-with-datasource migration:generate",
    "migration:up": "npm run build && npm run typeorm-with-datasource migration:run",
    "migration:down": "npm run typeorm-with-datasource migration:revert",
    "migration:drop": "npm run build && npm run typeorm-with-datasource schema:drop",
    "migration:test:up": "npm run typeorm-with-test-datasource migration:run",
    "migration:test:drop": "npm run build && npm run typeorm-with-test-datasource schema:drop",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "docker rm -f shops-test-postgres && ./run-test-database.sh && jest --runInBand --verbose --",
    "test:local": "npm run migration:test:drop && npm run migration:test:up && jest --verbose --runInBand --detectOpenHandles",
    "test:ci": "jest --verbose --runInBand --detectOpenHandles",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "argon2": "^0.30.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
